@model ExpatManager.Models.AgreementDetailDocumentUpload
@{
    ViewBag.Title = "Delete";
}
<h2>
    Delete Document</h2>
<h3>
    Are you sure you want to delete this document?</h3>
@Html.Partial("~/Views/Expatriate/_DisplayForControl.cshtml", Model.AgreementDetails.Expatriates)
@Html.Partial("../AgreementDetail/_DisplayForControl", Model.AgreementDetails)
@Html.Partial("_DisplayForControl", Model)
@Html.Partial("~/Views/Shared/DisplayTemplates/CreatedAndModifiedControl.cshtml", Model)
@using (Html.BeginForm())
{

    <p>
        <input type="submit" value="Delete" />
        |
        @Html.ActionLink("Back to Agreement Details", "Details", "AgreementDetail", new { id = Model.AgreementDetailID }, null)
    </p>
}
